{
  "name": "mini-js-parser",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "main": "dist/index.esm.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest --run",
    "build": "rolldown -c rolldown.config.ts",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix --deny-warnings",
    "lint:github": "oxlint --format=github",
    "fmt": "oxfmt --write",
    "fmt:check": "oxfmt --check",
    "lint:fmt": "pnpm run lint:fix && pnpm run fmt"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "rolldown": "1.0.0-rc.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "pnpm lint:fmt"
    ],
    "*.json": [
      "pnpm oxfmt"
    ]
  },
  "engines": {
    "pnpm": ">=10.14.0"
  },
  "packageManager": "pnpm@10.14.0"
}
